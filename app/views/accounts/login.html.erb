    <h1 class="h3 mb-3 fw-normal"></h1>
    <div class="field form-floating">
      <%= f.text_field :username, class: "form-control", id: "floatingInput", placeholder: "User Name"%>
      <%= f.label "username", placeholder: "username", for: "floatingInput"%>
    </div>


    <div class="field form-floating">
      <%= f.password_field :password, autocomplete: "current-password", id: "floatingPassword", placeholder: "Password", class: "form-control" %>
      <%= f.label :password, for: "floatingPassword" %>
    </div>

    <div class="actions">
      <%= f.submit "Log in", class: "w-100 btn btn-lg btn-primary" %>
    </div>
  <% end %>

	<br>

	<div class="modal fade" id="exampleModalCenteredScrollable" tabindex="-1" aria-labelledby="exampleModalCenterTitle" aria-hidden="true" style="display: none;">
	  <div class="modal-dialog modal-dialog-centered">
	    <div class="modal-content">
	      <div class="modal-header">
	        <h1 class="modal-title fs-5" id="exampleModalCenterTitle">Sign up</h1>
	        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
	      </div>
	      <div class="modal-body">
	        <%= render "form", account: @account %>
	    	</div>
	  	</div>
		</div>
	</div>

	<button type="button" class="w-100 btn btn-lg btn-success" data-bs-toggle="modal" data-bs-target="#exampleModalCenteredScrollable">
